{
    "name": "flexasus",
    "private": true,
    "license": "MIT",
    "devDependencies": {
      "patch-package": "^6.4.7"
    },
    "workspaces": [
      "apps/storybook-web",
      "apps/storybook-react-native",
      "packages/*"
    ],
    "scripts": {
      "dev:expo": "yarn workspace @flexasus/expo dev",
      "dev:next": "yarn workspace @flexasus/next dev",
      "dev:storybook:web": "yarn workspace @flexasus/storybook-web storybook",
      "dev:storybook:react-native": "yarn workspace @flexasus/storybook-react-native start",
      "build": "cd packages/next && yarn build",
      "deploy": "vercel --prod --no-clipboard",
      "postinstall": "patch-package"
    },
    "resolutions": {
      "babel-preset-expo": "^8.5.1"
    }
  }
  